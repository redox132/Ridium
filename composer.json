{
  "name": "ridium/ridium",
  "description": "The official Ridium application skeleton",
  "license": "MIT",
  "authors": [
    {
      "name": "redox132",
      "email": "ridaelkouri@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Ridium\\": "ridium/"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/redox132/framework"
    }
  ],
  "require": {
    "vlucas/phpdotenv": "^5.6",
    "ridium/framework": "^1.0",
    "jenssegers/blade": "^2.0",
    "fakerphp/faker": "^1.24"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "support": {
    "issues": "https://github.com/redox132/ridium/issues",
    "source": "https://github.com/redox132/ridium"
  },
  "keywords": [
    "php",
    "application",
    "skeleton",
    "ridium",
    "framework"
  ]
}